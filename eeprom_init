; main counter   
banksel REG4
    movlw   0x0
    movwf   REG4
    movlw   0x12
    call    Write
    call    eeprom_pause  
    banksel REG4
    movlw   0x0
    movwf   REG4
    movlw   0x13
    call    Write
    call    eeprom_pause
    banksel REG4
    movlw   0x0
    movwf   REG4
    movlw   0x14
    call    Write
    call    eeprom_pause 
    banksel REG4
    movlw   0x0
    movwf   REG4
    movlw   0x15
    call    Write
    call    eeprom_pause
; btn1
    banksel REG4
    movlw   0x01
    movwf   REG4
    movlw   0x12
    call    Write
    call    eeprom_pause  
    banksel REG4
    movlw   0x0A
    movwf   REG4
    movlw   0x13
    call    Write
    call    eeprom_pause
    banksel REG4
    movlw   0x0
    movwf   REG4
    movlw   0x14
    call    Write
    call    eeprom_pause 
    banksel REG4
    movlw   0x0
    movwf   REG4
    movlw   0x15
    call    Write
    call    eeprom_pause
; btn2
    banksel REG4
    movlw   0x02
    movwf   REG4
    movlw   0x12
    call    Write
    call    eeprom_pause  
    banksel REG4
    movlw   0x0A
    movwf   REG4
    movlw   0x13
    call    Write
    call    eeprom_pause
    banksel REG4
    movlw   0x0
    movwf   REG4
    movlw   0x14
    call    Write
    call    eeprom_pause 
    banksel REG4
    movlw   0x0
    movwf   REG4
    movlw   0x15
    call    Write
    call    eeprom_pause


loop  
    goto    loop
